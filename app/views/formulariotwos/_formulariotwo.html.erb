<div id="<%= dom_id formulariotwo %>">
  <p class="my-5">
    <strong class="block font-medium mb-1">Evidencia:</strong>
    <%= formulariotwo.evidencia %>
  </p>

  <% if action_name != "show" %>
    <%= link_to "Show this formulariotwo", formulariotwo, class: "rounded-lg py-3 px-5 bg-gray-100 inline-block font-medium" %>
    <%= link_to "Edit this formulariotwo", edit_formulariotwo_path(formulariotwo), class: "rounded-lg py-3 ml-2 px-5 bg-gray-100 inline-block font-medium" %>
    <hr class="mt-6">
  <% end %>
</div>
