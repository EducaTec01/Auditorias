<div id="<%= dom_id formulario %>">
  <p class="my-5">
    <strong class="block font-medium mb-1">Noauditoria:</strong>
    <%= formulario.noAuditoria %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Proceso:</strong>
    <%= formulario.proceso %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Fecha:</strong>
    <%= formulario.fecha %>
  </p>

  <% if action_name != "show" %>
    <%= link_to "Show this formulario", formulario, class: "rounded-lg py-3 px-5 bg-gray-100 inline-block font-medium" %>
    <%= link_to "Edit this formulario", edit_formulario_path(formulario), class: "rounded-lg py-3 ml-2 px-5 bg-gray-100 inline-block font-medium" %>
    <hr class="mt-6">
  <% end %>
</div>
